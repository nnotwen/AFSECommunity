{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "ChangelogFile",
	"type": "object",
	"properties": {
		"version": {
			"type": "string"
		},
		"changelog": {
			"type": "array",
			"items": {
				"type": "object",
				"properties": {
					"type": {
						"type": "string",
						"enum": ["added", "update", "removed"]
					},
					"timestamp": {
						"type": "number"
					},
					"list": {
						"type": "array",
						"items": {
							"type": "string"
						}
					}
				},
				"required": ["type", "timestamp", "list"],
				"additionalProperties": false
			}
		}
	},
	"required": ["version", "changelog"],
	"additionalProperties": false
}
